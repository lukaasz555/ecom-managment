model Staff {
  id             Int       @id @default(autoincrement())
  name           String
  lastname       String
  email          String    @unique
  phone          String
  password       String
  privileges     Json
  role           String
  createdAt      DateTime  @default(now())
  updatedAt      DateTime  @updatedAt
  deletedAt      DateTime?
  additionalNote String?
}
